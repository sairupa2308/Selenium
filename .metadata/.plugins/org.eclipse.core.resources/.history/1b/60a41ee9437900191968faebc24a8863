package JavaBasics;

public class WrapperClass {
   
      
	public static void main(String[] args) {
		// data conversation ------ string to int
		
          String x = "20";
          System.out.println(x+20);
          
        int i =  Integer.parseInt(x);// we are using this in many times
        System.out.println(i+20);
         
          
          
          
          
          
          
          
  }
  
}
