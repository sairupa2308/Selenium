package JavaBasics;

public class WrapperClass {
    String name = "tom";
      static int  age = 200;
      
	public static void main(String[] args) {
		// TODO Auto-generated method stub
          rupa();
        
          WrapperClass.rupa();
	}
  public void mail() {
	  System.out.println("mail method");
  }
  
  public static void rupa() {
	  System.out.println("rupa method");
  }
}
