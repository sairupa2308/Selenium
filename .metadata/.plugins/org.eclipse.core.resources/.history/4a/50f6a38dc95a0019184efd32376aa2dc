package OOPConcepts;

public class CallByValueandCallByRef {

	public static void main(String[] args) {
		
		CallByValueandCallByRef obj= new CallByValueandCallByRef() ;
				  int x=10;
				  int y=20;
				  obj.testSum(x,y);//callbyvalue and call y ref
				  
	}
	  
	public int testSum(int a,int b) {
		      a=30;
	          b=40;
	          int c=a+b;
	          return c;
		
	}
}
