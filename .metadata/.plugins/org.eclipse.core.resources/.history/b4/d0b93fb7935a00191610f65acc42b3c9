package OOPConcepts;

public class MethodOverloading {

	public static void main(String[] args) {
		
		MethodOverloading obj = new MethodOverloading();
		   
		obj.sum();
		obj.sum(10);
		
		
		
		
		
	}

	public static void sum() {
		System.out.println("sum of method");
	}
	
	public static void sum(int i) {
		System.out.println("sumof methods--1 param");
	}
	
	public static void mainsum(int k ,char l) {
		System.out.println("sum of methods--2 param");
	}
}
