package JavaBasics;

public class DataTypesInJava {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
            double d = 100;
            double d2 = 12;
            System.out.println(d);
            
            
            int a = 10;
            int b =20;
            
            String  x = "hai";
            String   y = "hello";
            
            System.out.println(a+b+x+y);
            System.out.println(x+y+a+b);
	}

}
