package OOPConcepts;

public class ONEDimArray {

	public static void main(String[] args) {
		    
		int i[]=new int[4];
		i[0]= 10;
		i[1]=20;
		i[2]=30;
		i[3]=40;
		System.out.println(i[2]);
		System.out.println(i[3]);
		System.out.println(i.length);
		
		for(int j=0;j<i.length;j++)
			System.out.println(j);
		

	}

}
